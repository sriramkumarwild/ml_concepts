   "source": [
    "def get_modelparams(X, y):\n",
    "    # w = (X^T * X)^-1 * X^T * y\n",
    "    w = np.dot(np.dot(inv(np.dot(X,X.T)),X),y)\n",
    "    \n",
    "    return w\n",
    "\n",
    "def get_polyfeatures(x, degree):\n",
    "    Data  = np.ones([len(x)])\n",
    "    for i in range(0,int(degree)):\n",
    "        Data  = np.append(Data, x**i)\n",
    "        Data  = np.resize(Data, (i+1,len(x)))\n",
    "    \n",
    "    return Data"
   ]
  },
