    "# First column consist of ones for the offset\n",
    "w     = []\n",
    "xeval = np.linspace(0,20,num=100)\n",
    "poly  = np.linspace(1,8, num=8)\n",
    "\n",
    "plt.plot(x,y, 'k*', lw=10, marker='o',markersize=5)\n",
    "plt.xlabel('x')\n",
    "plt.ylabel('sin(x)')\n",
    "plt.xlim((0, 12))\n",
    "plt.ylim((-1, 1))\n",
    "\n",
    "for degree in poly:\n",
    "    #Data  = np.append(Data, x**i)\n",
    "    #Data  = np.resize(Data, (i+1,len(x)))\n",
    "    w =[]\n",
    "    Data  = get_polyfeatures(x, degree)\n",
    "    w     = get_modelparams(Data,y)\n",
    "    # Plot regression line from the learnt model parameters\n",
    "    #Yout  = np.append(Yout, xeval**i)\n",
    "    Yout  = get_polyfeatures(xeval, degree)\n",
    "    #Yout  = np.resize(Yout, (i+1,len(xeval)))\n",
    "    yeval = np.dot(Yout.T, w)\n",
    "    plt.plot(xeval,yeval, 'g')\n",
